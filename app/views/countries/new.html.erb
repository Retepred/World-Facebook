<%= form_for @country do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name  %>
  <br>
  <%= f.label :flag_image %>
  <%= f.text_field :flag_image %>
  <br>
  <%= f.label :population %>
  <%= f.integer_field :population %>
  <br>
  <%= f.label :size %>
  <%= f.text_field :size %>
  <br>
  <%= f.label :language %>
  <%= f.text_field :language %>
  <br>
  <%= f.label :drinking_age %>
  <%= f.text_field :drinking_age %>
  <br>
  <%= f.submit 'Spawn Country!' %>
<% end %>